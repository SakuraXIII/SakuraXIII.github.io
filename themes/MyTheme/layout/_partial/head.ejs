<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <%
  var title = page.title;

  if (is_archive()){
    title = __('archive_a');

    if (is_month()){
      title += ': ' + page.year + '/' + page.month;
    } else if (is_year()){
      title += ': ' + page.year;
    }
  } else if (is_category()){
    title = __('category') + ': ' + page.category;
  } else if (is_tag()){
    title = __('tag') + ': ' + page.tag;
  }
  %>
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
  <title><%- theme.title %></title>
  <% if (theme.rss){ %>
  <link rel="alternate" href="<%= url_for(theme.rss) %>" title="<%= config.title %>" type="application/atom+xml">
  <% } %>
  <% if (theme.favicon){ %>
  <%- favicon_tag(theme.favicon) %> <% } %>
  <%- css('//at.alicdn.com/t/font_1317659_91ay27oeya8.css') %>
  <%- css('https://cdn.jsdelivr.net/npm/bootstrap@3.3.7/dist/css/bootstrap.min.css') %>
  <%- js('https://cdn.bootcss.com/html5shiv/3.7.3/html5shiv.min.js') %>
  <%- css('https://cdn.bootcss.com/animate.css/3.7.2/animate.min.css') %>
  <%- js('https://cdn.jsdelivr.net/npm/jquery@1.12.4/dist/jquery.min.js') %>
  <%- css('css/loader.css') %>
  <%- css('css/style') %>
  <%- css('css/myCss.css') %>
</head>